{
	"name": "t99/utils",
	"description": "Just a collection of useful PHP scripts, functions, and the like that I've bandaged together.",
	"version": "v0.0.1",
	"type": "library",
	"keywords": [
		"utilities",
		"array",
		"array-utilities",
		"functional"
	],
	"homepage": "https://github.com/T99/php-utils",
	"readme": "readme.md",
	"license": "GPL-3.0-or-later",
	"authors": [
		{
			"name": "Trevor Sears",
			"email": "trevor@trevorsears.com",
			"homepage": "https://trevorsears.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/T99/php-utils/issues",
		"forum": "https://github.com/T99/php-utils/discussions",
		"wiki": "https://github.com/T99/php-utils/wiki",
		"source": "https://github.com/T99/php-utils",
		"docs": "https://github.com/T99/php-utils/blob/master/readme.md"
	},
	"funding": [
		{
			"type": "buymeacoffee",
			"url": "https://www.buymeacoffee.com/trevorsears"
		}
	],
	"require-dev": {
		"phpunit/phpunit": "9.6.7"
	},
	"autoload": {
		"psr-4": {
			"T99\\Utils\\": "src/"
		},
		"files": [
			"src/Arrays/array_all.php",
			"src/Arrays/array_any.php",
			"src/Arrays/array_entries.php",
			"src/Arrays/array_find.php",
			"src/Arrays/array_flatten.php",
			"src/Arrays/array_head.php",
			"src/Arrays/array_largest_countable.php",
			"src/Arrays/array_map_assoc.php",
			"src/Arrays/array_none.php",
			"src/Arrays/array_stringify.php",
			"src/Arrays/array_tail.php",
			"src/Arrays/array_to_string.php",
			"src/Arrays/group-array.php",
			"src/Arrays/is_assoc.php",
			"src/Arrays/surround.php",
			"src/Formatting/casing-conversion.php",
			"src/Formatting/human-readable-bytes.php",
			"src/Formatting/indent.php",
			"src/Formatting/to-ascii-table.php",
			"src/Formatting/to_html_table.php",
			"src/Formatting/var_dump_to_string.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"T99\\Utils\\Tests\\": "tests/"
		}
	},
	"config": {
		"classmap-authoritative": true
	}
}
